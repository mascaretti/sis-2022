@article{duanBayesianConstraintRelaxation2020,
  title = {Bayesian Constraint Relaxation},
  author = {Duan, Leo L and Young, Alexander L and Nishimura, Akihiko and Dunson, David B},
  year = {2020},
  month = mar,
  journal = {Biometrika},
  volume = {107},
